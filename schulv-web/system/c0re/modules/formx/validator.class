class FormXValidator {
	var $form;
	var $data;

	function FormXValidator(&$form, $data) {
		$this->form = $form;
		$this->data = $data;
	}

	function validate() {
		user_error("FormXValidator::validate() has to be overloaded");
	}

	function store() {
		user_error("FormXValidator::store() has to be overloaded");
	}

	function import() {
		user_error("FormXValidator::import() has to be overloaded");
	}

};

